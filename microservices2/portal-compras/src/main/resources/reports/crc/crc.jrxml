	<parameter name="tipoDocumento" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT 
	cadastro.tipo_cadastro.nome AS tipo_cadastro_nome,
	endereco.endereco.logradouro,
	endereco.endereco.numero,
	fornecedor.fornecedor.razao_social,
	fornecedor.fornecedor.data_aprovacao,
	fornecedor.fornecedor.data_aprovacao + interval '1 year' AS data_vencimento,
	configuracao.pessoa.nome AS nome,
	configuracao.pessoa.documento
FROM fornecedor.fornecedor,
	cadastro.tipo_cadastro,
	configuracao.pessoa,
	endereco.endereco
WHERE 
	  ( 
	 cadastro.tipo_cadastro.id = fornecedor.fornecedor.tipo_cadastro_id 
	 AND configuracao.pessoa.id = fornecedor.fornecedor.usuario_id 
	 AND endereco.endereco.id = fornecedor.fornecedor.endereco_id 
	 AND fornecedor.fornecedor.id = $P{id} 
	 )]]>
	</queryString>
