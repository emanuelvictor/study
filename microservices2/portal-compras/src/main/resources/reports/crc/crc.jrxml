		<parameter name="id" class="java.lang.Long"/>
		<queryString>
			<![CDATA[SELECT 
		fornecedor.atividade_economica.code,
		fornecedor.atividade_economica.text
FROM 	
		fornecedor.fornecedor,
		fornecedor.atividade_economica_primaria,
 		fornecedor.atividade_economica_secundaria,
		fornecedor.atividade_economica
WHERE 
	    ( 
			fornecedor.fornecedor.id = $P{id} 
			AND 
			(
				(
					fornecedor.atividade_economica_secundaria.fornecedor_id = fornecedor.fornecedor.id
					AND fornecedor.atividade_economica_secundaria.atividade_economica_id = fornecedor.atividade_economica.code
				)
				OR
				(
					fornecedor.atividade_economica_primaria.fornecedor_id = fornecedor.fornecedor.id
					AND fornecedor.atividade_economica_primaria.atividade_economica_id = fornecedor.atividade_economica.code
				)
			)
	 )
GROUP BY fornecedor.atividade_economica.code, fornecedor.atividade_economica.text]]>
		</queryString>
		<field name="code" class="java.lang.String">
			<property name="com.jaspersoft.studio.field.label" value="code"/>
			<property name="com.jaspersoft.studio.field.tree.path" value="atividade_economica"/>
		</field>
		<field name="text" class="java.lang.String">
			<property name="com.jaspersoft.studio.field.label" value="text"/>
			<property name="com.jaspersoft.studio.field.tree.path" value="atividade_economica"/>
		</field>
	</subDataset>
