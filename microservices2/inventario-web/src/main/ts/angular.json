{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "sistema": {
      "root": "sistema",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "sistema",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "../resources/META-INF/resources/modules/",
            "index": "src/sistema/index.html",
            "main": "src/sistema/application/sistema-main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/sistema/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "./node_modules/@covalent/core/common/platform.scss",
              "src/sistema/application/sistema-theme.scss",
              "./node_modules/roboto-fontface/css/roboto/roboto-fontface.css",
              "src/assets/styles/nprogress.css",
              "src/assets/styles/ev-datepicker.scss"
            ],
            "scripts": [
              "src/assets/scripts/nprogress.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "sistema:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "sistema:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "sistema:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [],
            "exclude": []
          }
        }
      }
    },
    "portal": {
      "root": "portal",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "portal",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "../resources/META-INF/resources/modules/site/",
            "index": "src/portal/index.html",
            "main": "src/portal/application/portal-main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/portal/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "./node_modules/@covalent/core/common/platform.scss",
              "src/portal/application/portal-theme.scss",
              "./node_modules/roboto-fontface/css/roboto/roboto-fontface.css",
              "src/assets/styles/nprogress.css"
            ],
            "scripts": [
              "node_modules/nprogress/nprogress.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "portal:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "portal:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "portal:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [],
            "exclude": []
          }
        }
      }
    }
  },
  "defaultProject": "sistema"
}
